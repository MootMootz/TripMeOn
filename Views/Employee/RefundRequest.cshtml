@model List<TripMeOn.Models.PartnerProducts.Notification>
@{
    ViewBag.title = "RefoundRequest";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Refund Requests</h1>

<table class="table">
    <thead>
        <tr>
            <th>Message</th>
            <th>Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var notification in Model)
        {
            <tr>
                <td>@notification.Message</td>
                <td>@notification.CreatedAt</td>
            </tr>
        }
    </tbody>
</table>




